<c:FragmentDefinition
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	xmlns:f="sap.ui.layout.form"
>
	<Dialog
		id="openDialog"
		title="{i18n>discharge}"
	>
		<beginButton>
			<Button
				text="{i18n>save}"
				type="Accept"
				press="handleSaveMassDischarge"
			/>
		</beginButton>
		<endButton>
			<Button
				text="{i18n>cancel}"
				type="Reject"
				press="handleCancelMassDischarge"
			/>
		</endButton>
		<f:SimpleForm
			id="RegisterForm"
			editable="true"
		>
			<f:content>
				<!-- <Label text="{/#Customers/Cpf/@sap:label}"/>
				<MaskInput
					value="{Customer>/Cpf}"
					mask="999.999.999-99"
					placeholder="{i18n>addCPF}"
				/> -->
				<Label text="Parceiro"/>
				<Input
					id="Partner"
					valueHelpOnly="true"
					showValueHelp="true"
					valueHelpRequest="onPartnerF4"
				/>
				<Label text="Contrato"/>
				<Input
					id="Contract"
					valueHelpOnly="true"
					showValueHelp="true"
					valueHelpRequest="onContractF4"
				/>
				<Label text="Quantidade a baixar"/>
				<Input
					id="Quantity"
					type="Number"
				/>
				<Label text="Saldo"/>
				<Input
					id="Balance"  
					enabled="false"
					value="0.00"
				/>
			</f:content>
		</f:SimpleForm>
	</Dialog>
</c:FragmentDefinition>