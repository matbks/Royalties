<mvc:View
	controllerName="royalties.zroyalties.controller.Detail"
	xmlns="sap.m"
	xmlns:semantic="sap.f.semantic"
	xmlns:mvc="sap.ui.core.mvc"
>
	<semantic:SemanticPage
		id="detailPage"
		busy="{detailView>/busy}"
		busyIndicatorDelay="{detailView>/delay}"
	>
		<semantic:content>
			<smartTable:SmartTable
				xmlns:smartTable="sap.ui.comp.smarttable"
				xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1"
				entitySet="ZADOC_ROYALTIES_LOG"
				smartFilterId="sfb_production_processing"
				tableType="Table"
				header="{i18n>items}"
				persistencyKey="SmartTableAnalytical_Explored"
				class="sapUiResponsiveContentPadding"
				app:useSmartToggle="true"
				initiallyVisibleFields="Plant,Romaneio,EdcNumber,CreationDate,CreationTime,Discharge,FiscalYear,Balance,DischargeStatus"
				enableAutoBinding="true"
				showFullScreenButton="false"
				tableBindingPath="to_RoyaltiesLog"
				requestAtLeastFields="EdcNumber"
			>
				<Table
					id="list3"
					includeItemInSelection="true"
					contextualWidth="auto"
					autoPopinMode="true"
					sticky="ColumnHeaders"
					busyIndicatorDelay="{listView>/delay}"
					noDataText="{listView>/noDataText}"
					mode="SingleSelectMaster"
					growing="true"
					growingScrollToLoad="true"
				/>
			</smartTable:SmartTable>
		</semantic:content>
	</semantic:SemanticPage>
</mvc:View>